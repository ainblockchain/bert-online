{
  "name": "BERT Online (on Tensorflow)",
  "listed": true,
  "link": "https://github.com/ainblockchain/bert-online",
  "readme": "AIN.md",
  "branch": "master",
  "entry": "bert_online/run_squad_online.py",
  "executor": {
    "type": "exec_server",
    "exec_server_params": {
      "docker": {
        "image": "comcomai/bert-online:1.0.0",
        "work_dir": "/bert",
        "python_path": "/bert"
      },
      "exec_params": "--vocab_file=/bert/model_base_cased/vocab.txt  --bert_config_file=/bert/model_base_cased/bert_config.json  --init_checkpoint=/bert/squad1.1-output-train_2/model.ckpt-14599  --max_seq_length=320  --doc_stride=128  --n_best_size=3"
    }
  }
}
